{
  "name": "codegraph",
  "displayName": "CodeGraph",
  "description": "Get the picture",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codegraph.helloWorld",
    "onCommand:codegraph.foldersMap",
    "onCommand:codegraph.filesMap"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [{
      "command": "codegraph.helloWorld",
      "title": "Hello World"
    },
    {
      "command": "codegraph.foldersMap",
      "title": "Show Opened Folders"
    },
    {
      "command": "codegraph.filesMap",
      "title": "Show files map"
    }
  ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.32.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
